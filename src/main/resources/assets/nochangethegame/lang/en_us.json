  "text.autoconfig.nochangethegame.option.movementInterpolation": "Movement Interpolation",
  "text.autoconfig.nochangethegame.option.movementInterpolation.disableForSneaking": "Disable smooth sneaking animation",
  "text.autoconfig.nochangethegame.option.movementInterpolation.disableForSneaking.@Tooltip[0]": "Reverts the smooth animation when sneaking to that",
  "text.autoconfig.nochangethegame.option.movementInterpolation.disableForSneaking.@Tooltip[1]": " seen in older versions of Minecraft.",
  "text.autoconfig.nochangethegame.option.movementInterpolation.disableForHeads": "Disable head rotation animation",
  "text.autoconfig.nochangethegame.option.movementInterpolation.disableForHeads.@Tooltip[0]": "Reverts the smooth animation of head movements to that",
  "text.autoconfig.nochangethegame.option.movementInterpolation.disableForHeads.@Tooltip[1]": " seen in older versions of Minecraft.",
  
  "text.autoconfig.nochangethegame.option.swordBlocking": "Sword Blocking",
  "text.autoconfig.nochangethegame.option.swordBlocking.hideShields": "Hide held shields",
  "text.autoconfig.nochangethegame.option.swordBlocking.hideShields.@Tooltip[0]": "Turns all held shields invisible.",
  "text.autoconfig.nochangethegame.option.swordBlocking.hideShields.@Tooltip[1]": "This includes shields held by other players.",
  "text.autoconfig.nochangethegame.option.swordBlocking.hideOffhandSlot": "Hide offhand slot",
  "text.autoconfig.nochangethegame.option.swordBlocking.hideOffhandSlot.@Tooltip[0]": "Hides the offhand slot in your hotbar and disables the offhand swap key.",
  "text.autoconfig.nochangethegame.option.swordBlocking.hideOffhandSlot.@Tooltip[1]": "§7(You can still access your offhand in your main inventory screen)",
  "text.autoconfig.nochangethegame.option.swordBlocking.animateSword": "Enable sword blocking",
  "text.autoconfig.nochangethegame.option.swordBlocking.animateSword.@Tooltip[0]": "Blocking with a sword will rotate the sword",
  "text.autoconfig.nochangethegame.option.swordBlocking.animateSword.@Tooltip[1]": " as seen in versions prior to 1.9.",
  "text.autoconfig.nochangethegame.option.swordBlocking.preventCombinedAnimation": "Fix Combined Animation",
  "text.autoconfig.nochangethegame.option.swordBlocking.preventCombinedAnimation.@Tooltip[0]": "Prevents the blocking and swinging animations from playing together.",
  "text.autoconfig.nochangethegame.option.swordBlocking.preventCombinedAnimation.@Tooltip[1]": "Disabling this setting will allow for some extreme 1.7-like blocking animations.",
  "text.autoconfig.nochangethegame.option.swordBlocking.fakeShield": "Remove blocking latency",
  "text.autoconfig.nochangethegame.option.swordBlocking.fakeShield.@Tooltip[0]": "Avoids the delay when blocking with a sword by keeping an invisible",
  "text.autoconfig.nochangethegame.option.swordBlocking.fakeShield.@Tooltip[1]": " fake shield in your offhand whenever a sword is being held.",
  
  "text.autoconfig.nochangethegame.option.itemCooldowns": "Item Cooldowns",
